{
  "name": "@glenstack/api",
  "private": true,
  "main": "dist/worker.js",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml",
    "postinstall": "npm run generate",
    "prebuild": "npm run generate",
    "build": "ts-node scripts/build.ts",
    "lint": "eslint --ignore-path .gitignore --max-warnings=0 'src/**/*'",
    "precheck-types": "npm run generate",
    "check-types": "tsc",
    "prettier": "prettier --config ../../.prettierrc --ignore-path .gitignore '**/*.(js|ts|json|md)'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "precommit": "npm run check-types && lint-staged && npm run build && npm run coverage",
    "validate": "concurrently 'npm:check-types' 'npm:check-format' 'npm:lint' 'npm:build' 'npm:coverage'",
    "test": "jest",
    "coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- -o --watch",
    "start": "wrangler dev",
    "start:data": "ts-node src/data/test_data.ts",
    "start:https": "wrangler dev --local-protocol https",
    "voyager": "ts-node scripts/voyager.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "2.2.2",
    "@cloudflare/wrangler": "1.16.1",
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-resolvers": "1.19.2",
    "@luckycatfactory/esbuild-graphql-loader": "3.6.0",
    "@types/jest": "26.0.23",
    "@types/node": "15.12.2",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "apollo-server": "2.24.0",
    "concurrently": "6.1.0",
    "esbuild": "0.12.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "express": "4.17.1",
    "graphql-voyager": "1.0.0-rc.31",
    "http-proxy-middleware": "2.0.0",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "utility-types": "3.10.0"
  },
  "dependencies": {
    "@giraphql/core": "2.6.0",
    "@glenstack/cf-workers-graphql": "1.0.4",
    "@graphql-tools/schema": "6.0.18",
    "await-to-js": "3.0.0",
    "cross-fetch": "3.1.4",
    "faunadb": "4.2.0",
    "graphql": "15.3.0",
    "graphql-tag": "2.11.0",
    "toucan-js": "2.5.0-beta.1",
    "uuid": "8.3.2"
  }
}
