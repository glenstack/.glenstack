{
  "name": "@glenstack/glenstack.com",
  "private": true,
  "dependencies": {
    "@fontsource/inter": "4.3.0",
    "@headlessui/react": "1.1.1",
    "@heroicons/react": "1.0.1",
    "@sentry/react": "6.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "build": "REACT_APP_GIT_SHA=$(git rev-parse HEAD) craco build",
    "lint": "eslint --ignore-path .gitignore --max-warnings=0 'src/**/*.ts?(x)'",
    "check-types": "tsc",
    "prettier": "prettier --config ../../.prettierrc --ignore-path .gitignore '**/*.(js|ts|json|md)'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "precommit": "npm run check-types && lint-staged && npm run build && npm run coverage",
    "validate": "concurrently 'npm:check-types' 'npm:check-format' 'npm:lint' 'npm:build' 'npm:coverage'",
    "test": "craco test --watchAll=false",
    "coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- -o --watch",
    "eject": "react-scripts eject",
    "start": "craco start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cloudflare/wrangler": "1.16.1",
    "@craco/craco": "6.1.2",
    "@tailwindcss/forms": "0.3.2",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.8",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.2",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.7",
    "autoprefixer": "9.8.6",
    "concurrently": "6.1.0",
    "eslint-config-prettier": "8.3.0",
    "lint-staged": "11.0.0",
    "postcss": "7.0.35",
    "postcss-cli": "7.1.2",
    "prettier": "2.2.1",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.1.2",
    "typescript": "4.2.4"
  }
}
