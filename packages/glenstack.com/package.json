{
  "name": "@glenstack/glenstack.com",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.19",
    "@fontsource/jost": "4.3.0",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@headlessui/react": "1.2.0",
    "@heroicons/react": "1.0.1",
    "@sentry/react": "6.4.0",
    "classnames": "2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-router-dom": "5.2.0",
    "react-schemaorg": "1.3.1",
    "schema-dts": "0.8.2",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "prebuild": "npm run codegen",
    "build": "REACT_APP_GIT_SHA=$(git rev-parse HEAD) craco build",
    "lint": "eslint --ignore-path .gitignore --max-warnings=0 'src/**/*.ts?(x)'",
    "codegen": "graphql-let --config .graphql-let.production.yml --require dotenv/config",
    "precheck-types": "npm run codegen",
    "check-types": "tsc",
    "prettier": "prettier --config ../../.prettierrc --ignore-path .gitignore '**/*.(js|ts|json|md)'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "precommit": "npm run check-types && lint-staged && npm run build && npm run coverage",
    "validate": "concurrently 'npm:check-types' 'npm:check-format' 'npm:lint' 'npm:build' 'npm:coverage'",
    "test": "craco test --watchAll=false",
    "coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- -o --watch",
    "eject": "react-scripts eject",
    "start": "craco start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cloudflare/wrangler": "1.16.1",
    "@craco/craco": "6.1.2",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/plugin-helpers": "1.18.6",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@tailwindcss/forms": "0.3.2",
    "@tailwindcss/typography": "0.4.0",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26.0.23",
    "@types/node": "15.3.0",
    "@types/react": "17.0.6",
    "@types/react-dom": "17.0.5",
    "@types/react-helmet": "6.1.1",
    "@types/react-router-dom": "5.1.7",
    "autoprefixer": "9.8.6",
    "concurrently": "6.1.0",
    "eslint-config-prettier": "8.3.0",
    "graphql-let": "0.18.0-beta.4",
    "lint-staged": "11.0.0",
    "postcss": "7.0.36",
    "postcss-cli": "7.1.2",
    "prettier": "2.3.0",
    "react-scripts": "4.0.3",
    "remark-frontmatter": "3.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.1.2",
    "typescript": "4.2.4"
  }
}
